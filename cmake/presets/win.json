{
  "version": 8,
  "$schema": "https://cmake.org/cmake/help/latest/_downloads/3e2d73bff478d88a7de0de736ba5e361/schema.json",
  "include": ["base.json", "arch.json", "os.json", "config.json"],
  "configurePresets": [
    {
      "name": "win-x64-debug",
      "displayName": "win-x64-debug",
      "inherits": ["debug", "x64", "win", "base"]
    },
    {
      "name": "win-x64-release",
      "displayName": "win-x64-release",
      "inherits": ["release", "x64", "win", "base"]
    },
    {
      "name": "win-x86-debug",
      "displayName": "win-x86-debug",
      "inherits": ["debug", "x86", "win", "base"]
    },
    {
      "name": "win-x86-release",
      "displayName": "win-x86-release",
