{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 27,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "base",
      "hidden": true,
      "binaryDir": "${sourceDir}/build/${presetName}",
      "errors": {
        "dev": true
      },
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "CMAKE_PROJECT_TOP_LEVEL_INCLUDES": "${sourceDir}/cmake/conan.cmake",
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/dist/${presetName}"
      }
    },
    {
      "name": "x64",
      "hidden": true,
      "cacheVariables": {
        "OPT_ARCH": "x64"
      }
    },
    {
      "name": "debug",
      "hidden": true,
      "cacheVariables": {
        "OPT_CONFIG": "debug",
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "release",
      "hidden": true,
      "cacheVariables": {
        "OPT_CONFIG": "release",
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "win-x64",
      "hidden": true,
      "inherits": ["x64"],
      "architecture": {
        "strategy": "set",
        "value": "x64"
      }
    },
    {
      "name": "win-x64-debug",
      "inherits": ["win-x64", "debug", "base"],
      "displayName": "Windows Debug (x64)"
    },
    {
      "name": "win-x64-release",
      "inherits": ["win-x64", "release", "base"],
      "displayName": "Windows Release (x64)"
    }
  ],
  "buildPresets": [
    {
      "name": "win-x64-debug",
      "displayName": "Windows Debug (x64)",
      "configurePreset": "win-x64-debug",
      "configuration": "Debug",
      "jobs": 8
    },
    {
      "name": "win-x64-release",
      "displayName": "Windows Release (x64)",
      "configurePreset": "win-x64-release",
      "configuration": "Release",
      "jobs": 8
    }
  ],
  "workflowPresets": [
    {
      "name": "asdasd",
      "description": "aaaa",
      "displayName": "aaaa",
      "steps": [
        {
          "type": "configure",
          "name": "win-x64-debug"
        }
      ]
    }
  ]
}
