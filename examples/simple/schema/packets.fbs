namespace my.protocol;

attribute "packet";

enum AccountStatus : uint8 {
  Active,
  Unconfirmed,
  Blocked,
}

table AuthRequest (packet) {
  email: string;
  password: string;
}

table AuthResponse (packet) {
  success: bool;
  status: AccountStatus;
}

// Lua script templates are filtering only tables marked as packet
table ThisIsNotAPacket {
  test: string;
}
